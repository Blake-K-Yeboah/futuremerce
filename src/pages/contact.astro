---
import Layout from '../layouts/Layout.astro';
import Breadcrumbs from '../components/Breadcrumbs.astro';
import Form from '../components/Contact/Form.astro';
import BookACall from '../components/Contact/BookACall.astro';

const links = [
    {
        label: 'Home',
        href: '/'
    },
    {
        label: 'Contact Us'
    }
]
---

<Layout title="Futuremerce - Contact Us" metaInfo={{ canonical: "https://www.futuremerce.com/contact/" }}>
    <Breadcrumbs links={links} />
    <Form />
    <BookACall />
</Layout>